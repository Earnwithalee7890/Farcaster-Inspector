# Neynar API Key (Get one at https://neynar.com/)
NEYNAR_API_KEY=your_neynar_api_key_here

# Dune API Key (Get one at https://dune.com/settings/api)
DUNE_API_KEY=your_dune_api_key_here

# Talent Protocol API Key (Optional - https://passport.talentprotocol.com/)
TALENT_PROTOCOL_API_KEY=your_talent_api_key_here

# Quotient API Key (Get one at https://quotient.social)
QUOTIENT_API_KEY=your_quotient_api_key_here

# App URL (for production)
NEXT_PUBLIC_APP_URL=https://your-app.vercel.app

# Farcaster Manifest Signature (Generate at https://warpcast.com/~/developers/frames)
# These are required for authenticated frame actions
FARCASTER_MANIFEST_HEADER=
FARCASTER_MANIFEST_PAYLOAD=
FARCASTER_MANIFEST_SIGNATURE=

# Optional: Port
PORT=3000